{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ADF-krisha-DW"
		},
		"Snowflake1_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'Snowflake1'"
		},
		"SqlServer1_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'SqlServer1'"
		},
		"LS_Snowflake_SAS_sasUri": {
			"type": "secureString",
			"metadata": "Secure string for 'sasUri' of 'LS_Snowflake_SAS'"
		},
		"Snowflake1_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "@{linkedService().Database}"
		},
		"Snowflake1_properties_typeProperties_role": {
			"type": "string",
			"defaultValue": "@{linkedService().Role}"
		},
		"Snowflake1_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "@{linkedService().Host}"
		},
		"SqlServer1_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "@{linkedService().FullyQualifiedDomainName}"
		},
		"SqlServer1_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "DAMG7370FALL2025"
		},
		"SqlServer1_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "@{linkedService().Username}"
		},
		"ls_azure_key_vault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://krisha-keyvault.vault.azure.net/"
		},
		"ls_azure_storage_blob_properties_typeProperties_serviceEndpoint": {
			"type": "string",
			"defaultValue": "https://krishastoragedamg.blob.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ADF-krisha-DW_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"Snowflake1_password": {
						"value": "[parameters('Snowflake1_password')]"
					},
					"SqlServer1_password": {
						"value": "[parameters('SqlServer1_password')]"
					},
					"LS_Snowflake_SAS_sasUri": {
						"value": "[parameters('LS_Snowflake_SAS_sasUri')]"
					},
					"Snowflake1_properties_typeProperties_database": {
						"value": "[parameters('Snowflake1_properties_typeProperties_database')]"
					},
					"Snowflake1_properties_typeProperties_role": {
						"value": "[parameters('Snowflake1_properties_typeProperties_role')]"
					},
					"Snowflake1_properties_typeProperties_host": {
						"value": "[parameters('Snowflake1_properties_typeProperties_host')]"
					},
					"ls_azure_key_vault_properties_typeProperties_baseUrl": {
						"value": "[parameters('ls_azure_key_vault_properties_typeProperties_baseUrl')]"
					},
					"ls_azure_storage_blob_properties_typeProperties_serviceEndpoint": {
						"value": "[parameters('ls_azure_storage_blob_properties_typeProperties_serviceEndpoint')]"
					},
					"SqlServer1_properties_typeProperties_server": {
						"value": "[parameters('SqlServer1_properties_typeProperties_server')]"
					},
					"SqlServer1_properties_typeProperties_database": {
						"value": "[parameters('SqlServer1_properties_typeProperties_database')]"
					},
					"SqlServer1_properties_typeProperties_userName": {
						"value": "[parameters('SqlServer1_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}